        <script type="text/javascript">
            $(document).ready(function () {
                $('.i-a-bh').on($.clickAction, function () {
                    var tg = $('.i-bh-tab');
                    if (tg.hasClass('on')) {
                        tg.removeClass('on');
                    }
                    else {
                        tg.addClass('on');
                    }
                });
                $('.i-a-back').on($.clickAction, function () {
                    window.location = window.sessionStorage.backurl;
                });
                SetTotal();
            });
            

            function deleteCartWare(id) {                
                if (confirm("确定要删除吗？")) {
                    $("#product" + id).remove();
                }
            }
            function changeSelected(e,id) {                
                var obj = $(e);
                if (obj.hasClass("checked")) {
                    obj.removeClass("checked");
                }
                else {
                    obj.addClass("checked");
                }
                SetTotal();
            }
            function checkAllProduct(e) {
            
                var obj = $(e);
                if (obj.hasClass("checked")) {
                    $(".cart-checkbox.checked").each(function () {
                        $(this).removeClass("checked");
                    })
                    obj.removeClass("checked");
                }
                else {
                    $(".cart-checkbox").each(function () {
                        $(this).addClass("checked");
                    })
                    obj.addClass("checked");
                }
                SetTotal();
            }
            function SetTotal() {
                var sumPrice = cartSumPrice();
                $("#cart_realPrice").html(sumPrice);

                //商品总额
                $("#cart_oriPrice").html(sumPrice);
                //返现
                $("#cart_rePrice").html("0.00");
                //结算数量
                var iCount = 0;
                $("#notEmptyMyCart .quantity").each(function () {
                    var q = $(this).val();
                    if (q) {
                        q = parseInt(q);
                    }
                    else {
                        q = 0;
                    }
                    iCount += q;
                })
                $("#checkedNum").html(iCount);
                //设置全选按钮状态
                var notCheckCount = 0;
                $("#notEmptyMyCart .cart-checkbox").each(function () {
                    if (!$(this).hasClass("checked")) {
                        notCheckCount++;
                    }
                })
                if (notCheckCount > 0) {
                    $("#payment .cart-checkbox.checked").each(function () {
                        $(this).removeClass("checked");
                    })
                }
                else {
                    $("#payment .cart-checkbox").each(function () {
                        $(this).addClass("checked");
                    })
                }
            }
            function cartSumPrice() {
                var sumTotal = 0.00;
                $("#notEmptyMyCart .cart-checkbox.checked").each(function () {                    
                    var productId = $(this).attr("id").replace("checkIcon","");
                    var price = $("#price" + productId).text().replace("￥","");
                    if (price) {
                        price = parseFloat(price);
                    }
                    else {
                        price = 0.00;
                    }
                    var quantity = $("#num" + productId).val();
                    if (quantity) {
                        quantity = parseFloat(quantity);
                    }
                    else {
                        quantity = 0.00;
                    }
                    sumTotal += price * quantity;
                })
                return sumTotal;

            }
            function addQuantityBybutton(productid) {
                var q = $("#num" + productid).val();
                if (q) {
                    q = parseInt(q);
                    q++;
                }
                $("#num" + productid).val(q);
                SetTotal();
            }
            function subQuantityBybutton(productid) {
                var q = $("#num" + productid).val();
                if (q) {
                    q = parseInt(q);
                    if (q > 1) {
                        q--;
                    }
                }
                $("#num" + productid).val(q);
                SetTotal();
            }
           
        </script>
