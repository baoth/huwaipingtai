    <script type="text/javascript">
        $(document).ready(function () {
            $('.i-a-bh').on('click', function () {
                var tg = $('.i-bh-tab');
                if (tg.hasClass('on')) {
                    tg.removeClass('on');
                }
                else {
                    tg.addClass('on');
                }
            });
        });
