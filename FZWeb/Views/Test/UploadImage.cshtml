        <script type="text/javascript">
            $(document).ready(function () {               

               
                BindEvent();
               
            });

           
            var BindEvent = function () {
                //上传
                $("#btn_upload").bind("click", function () {
                    if (CheckUpload()) {
                        form.submit();
                    }
                });
            }

            var CheckUpload = function () {
           
                var value = $("#imageUpLoad").val();
                if (value == "") {
                    alert("请选择要上传的图片！");
                    return false;
                }
                return true;
            }
           
        </script>

       
