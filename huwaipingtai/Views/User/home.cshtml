@{
    Layout = null;
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0" />
        <meta name="apple-mobile-web-app-capable" content="yes" />
        <meta name="apple-mobile-web-app-status-bar-style" content="black" />
        <script src="../../Scripts/qsm-mui/fx.js" type="text/javascript"></script>
        <script src="../../Scripts/qsm-mui/zepto-1.1.4.min.js" type="text/javascript"></script>
        <script src="../../Scripts/qsm-mui/zepto.selector.js" type="text/javascript"></script>
        <script src="../../Scripts/qsm-mui/zepto.touch.js" type="text/javascript"></script>
        
        <style type="text/css">
        body {
        min-width: 320px;
        font-size: 1em;
        font-family: 'microsoft yahei',Verdana,Arial,Helvetica,sans-serif;
        color: #000;
        -webkit-text-size-adjust: none;
        }
        
        .user-bg {
        height: 186px;
        z-index: -1;
        position: absolute;
        left: 0;
        width: 100%;
        }
        
        .user-wrapper {
        overflow: hidden;
        }
        
        .user {
        height: 94px;
        margin-top: 20px;
        }
        
        .user-avatar {
        -webkit-border-radius: 8px;
        float: left;
        display: block;
        width: 70px;
        height: 70px;
        border: 2px solid #fff;
        margin-left: 30px;
        }
        
        .user-info {
        float: left;
        margin-left: 24px;
        }
        
        .user-name, .user-level, .user-point {
        display: block;
        color: #fff;
        line-height: 25px;
        }
        .user-level, .user-point {
        opacity: .7;
        }
        
        .user-name, .user-level, .user-point {
        display: block;
        color: #fff;
        line-height: 25px;
        }
        a {
        color: #000;
        text-decoration: none;
        }
        
        .msg {
        position: absolute;
        display: block;
        padding-top: 60px;
        right: 17px;
        }
        
        .msg:after {
        content: '';
        background: url(../../Content/images/msg-icon2.png) no-repeat;
        background-size: 100%;
        width: 20px;
        height: 20px;
        display: inline-block;
        float: right;
        margin-left: 5px;
        }
        </style>
    </head>
    <body>
        @Html.Partial("_HeaderTypical", new ViewDataDictionary { { "title", "我的账户" } })
       
        <div class="user-bg" style="background:url(../../Content/images/bgdefault.png) no-repeat;background-size:cover"></div>
        <div class="user-wrapper">
	        <div class="div-user">
                <div class="user">
    		         <img src="../../Content/images/userdefaul.png" class="user-avatar">
                      <div class="user-info">
                        <span class="user-name">tzweilaiok</span>   
                        <span class="user-level">铜牌用户</span>   
                      </div>
    		        <a href="/user/messages.action?sid=2522b1a01c942a1cb55c5a635f2cb57d">
    			        <span class="msg">消息</span>
    		        </a>
                </div>
		   </div>

        </div>

        <div class="shortcut-navs">

          <ul class="shortcut-nav">
            <li>
			    <a class="link link1" href="/user/waite4Payment.action?sid=2522b1a01c942a1cb55c5a635f2cb57d">
				    待付款
				</a>
		    </li>
            <li>
			    <a class="link link2" href="/user/waitDeliveryOrderList.action?sid=2522b1a01c942a1cb55c5a635f2cb57d">
				    待收货
									    <span class="tip_num">1</span>
							    </a>
		    </li>
            <li>
			    <a class="link link3" href="/user/allOrders.action?sid=2522b1a01c942a1cb55c5a635f2cb57d">
				    全部订单
			    </a>
		    </li>
			<li>
        			<a class="link link5" href="/user/jingBeanBalance.action?sid=2522b1a01c942a1cb55c5a635f2cb57d">
        				我的资产
        			</a>
        	</li>
		 </ul>

	      <ul class="shortcut-nav">
		     <li>
			    <a class="link link6" href="/user/accountCenter.action?sid=2522b1a01c942a1cb55c5a635f2cb57d">
				    帐户中心
			    </a>
		    </li>
            <li>
			    <a class="link link7" href="/user/services.action?sid=2522b1a01c942a1cb55c5a635f2cb57d">
				    客户服务
			    </a>
		    </li>
			  <li>
    			    <a class="link link8" href="/user/preSells.action?sid=2522b1a01c942a1cb55c5a635f2cb57d">
    				    我的预约
    			    </a>
    		  </li>
			  <li>
        	  </li>
		   </ul>

        </div>
        @Html.Partial("_FooterTypical")
