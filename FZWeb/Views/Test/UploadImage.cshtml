    <style type="text/css">
        *
        {
            margin: 0;
            padding: 0;   
         }
        .box {
            position: relative;           
        }
        section, nav, aside, menu {
            display: block;
        }
        .box ul {
            overflow: hidden;
            padding: 0 5px;
            width: 100%;
            z-index: 2;
            box-sizing: border-box;
        }
        ol, ul, li {
            list-style: none;
        }
        .box li:nth-child(3n+1) {
            clear: both;
        }
        .box2 li {
             padding-bottom: 5px;
        }
        .box li {
            float: left;
            display: block;
            width: 33.333%;
            padding: 0 5px;
            box-sizing: border-box;
            margin-top: 10px;
        }
        .box li a {
        width: 100%;
        }
        a:link, a:visited, a:hover, a:active {
            color: #727272;
        }
        a {
            text-decoration: none!important;
            color: #727272;
            outline: none;
        }
        .box li span:nth-child(1) {
            font-size: 0;
            
            display: block;
        }
        .box li img {
            width: 100%;
            min-height: 66px;
        }
        .box li span em {
            font-size: 0.75rem;
            position: absolute;
            left: 0;
            bottom: 0;
            display: block;
            width: 100%;
            padding-right: 4px;
            color: #FFFFFF;
            text-align: right;
            background: rgba(0,0,0,0.5);
            box-sizing: border-box;
        }
        
    </style>
  
    @*上传样式*@    
    <style type="text/css">
    .divUpload 
    {
        padding: 20px 10px;
        border: 1px solid #c8c7cc;
        margin: 10px;
    }
   
    </style>
    </head>
    <body>
        @Html.Partial("_HeaderTypical", new ViewDataDictionary { { "title", "上传商品图片" } })
         <div class="divUpload">
                <form id="form" runat="server" action="/Test/FileUpLoad" method="post" enctype="multipart/form-data">
                    
                    
                    <table>
                        <tr>
                            <td><input type="file" id="imageUpLoad" name="imageUpLoad" /></td>
                            <td></td>
                        </tr>
                        <tr>
                            <td>图片宽度:</td>
                            <td style="text-align:left"><input type="text" name="imgWidth" id="txt_width" /></td>
                        </tr>
                        <tr>
                            <td>图片高度:</td>
                            <td style="text-align:left"><input type="text" name="imgHeigth" id="txt_height" /></td>
                        </tr>
                        <tr>
                            <td><input type="button"  id="btn_upload" value="上传" />   </td>
                            <td></td>
                        </tr>
                    </table>
                     <div style="display:none">
                      <input type="hidden" id="txt_sku" name="Sku" value="@ViewData["Sku"]" />
                      <input type="hidden" id="txt_shangpinid" name="ShangPinId" value="@ViewData["ShangPinId"]" />
                      <input  type="hidden" id="txt_imgPath" name="ImagPath" value="@ViewData["imgPath"]"/>
        
                    </div>              
                </form> 
         </div>

         <div id="wrapper" style="display:block">
            
            <section class="box box2">
	          <ul id="RowContent" >
              </ul>  
             </section> 
         </div>

     
      
    </body>

