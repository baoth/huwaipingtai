            }
            .shortcut-nav .link6:before {
                display: block;
                content: '';
                height: 25px;
                width: 25px;
                background: url(../../Content/images/shortcut-nav-icons.png) no-repeat;
                background-size: 221px 25px;
                background-position: -147px 0;
               
