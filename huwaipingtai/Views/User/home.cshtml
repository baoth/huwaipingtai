        @*功能样式*@
        <style type="text/css">
            .shortcut-navs {
            font-size: 14px;
            }
            .shortcut-nav {
               
                background: #f1f1f5;
                width: 100%;
                display: block;
            }
            ul, ol {
            list-style: none;
            }
            .shortcut-nav:first-child li {
                border-bottom: 0;
            }
            .shortcut-nav li {
                border-top: 1px solid #c8c7cc;
                border-bottom: 1px solid #c8c7cc;
                background: #fff;
                text-align: left;
                width: 100%;
                overflow: hidden;                
                display: block;
                height: 70px;
               
            }
            .shortcut-nav * {
                -webkit-tap-highlight-color: rgba(0,0,0,0);
                -webkit-box-sizing: border-box;
                -moz-box-sizing: border-box;
                -ms-box-sizing: border-box;
                box-sizing: border-box;
            }
            
            .shortcut-nav .link {
                display: inline-block;
                width: 100%;
                float: left;
                margin-left: 20px;
                position: relative;
            }
            .link .tip_num {
                color: #fff;
                display: block;
                background: #e4393c;
                width: 16px;
                height: 16px;
                position: absolute;
                top: -1px;
                left: 16px;
                -webkit-border-radius: 9px;
                font-size: 10px;
                line-height: 17px;
                text-align: center;
            }
            a {
                color: #000;
                text-decoration: none;
                top: 15px;
            }
            .shortcut-nav .link1:before {
                display: block;
                content: '';
                height: 25px;
                width: 25px;
                background: url(../../Content/images/shortcut-nav-icons.png) no-repeat;
                background-size: 222px 25px;
                background-position: 0 0;
               
            }
           .shortcut-nav .link2:before {
                display: block;
                content: '';
                height: 25px;
                width: 25px;
                background: url(../../Content/images/shortcut-nav-icons.png) no-repeat;
                background-size: 222px 25px;
                background-position: -25px 0;
               
            }
            .shortcut-nav .link3:before {
                display: block;
                content: '';
                height: 25px;
                width: 25px;
                background: url(../../Content/images/shortcut-nav-icons.png) no-repeat;
                background-size: 223px 25px;
                background-position: -50px 0;
               
            }
            .shortcut-nav .link5:before {
                display: block;
                content: '';
                height: 25px;
                width: 25px;
                background: url(../../Content/images/shortcut-nav-icons.png) no-repeat;
                background-size: 222px 25px;
